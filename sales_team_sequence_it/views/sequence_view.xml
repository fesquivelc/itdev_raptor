<odoo>
    <data>
        <record id="sequence_view_annotation" model="ir.ui.view">
            <field name="name">sequence.view.annotation</field>
            <field name="model">ir.sequence</field>
            <field name="inherit_id" ref="base.sequence_view" />
            <field name="arch" type="xml">
                <xpath expr="//page/group[@col='3']/group[2]" position="inside">
                    <label colspan="2" string="Equipo de ventas: %%(sale_team)s"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>